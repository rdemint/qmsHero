<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="clr-namespace:QmsDoc.Controls;assembly=QmsDoc">
    <DataTemplate
        DataType="{x:Type controls:ControlTextBox}">
        <StackPanel Orientation="Horizontal">
            <Label Content="{Binding Path=DocActionName}"/>
            <TextBox MinWidth="200" Text="{Binding Path=DocActionVal}"/>
        </StackPanel>
    </DataTemplate>
    
</ResourceDictionary>