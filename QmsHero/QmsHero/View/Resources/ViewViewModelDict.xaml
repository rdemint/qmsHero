<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:QmsHero.ViewModel"
                    xmlns:view="clr-namespace:QmsHero.View"
                    >
    <DataTemplate DataType="{x:Type vm:MainViewModel}">
        <view:MainWindow />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ConfigViewModel}">
        <view:ConfigView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CustomProcessingViewModel}">
        <view:CustomProcessingView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ResultsViewModel}">
        <view:ResultsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:QuickActionsViewModel}">
        <view:QuickActionsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:FilePropertiesViewModel}">
        <view:FilePropertiesView/>
    </DataTemplate>
</ResourceDictionary>